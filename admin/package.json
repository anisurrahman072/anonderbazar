  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4000",
    "prebuild": "ng lint --fix",
    "build": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build  --source-map",
    "build:prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --aot",
